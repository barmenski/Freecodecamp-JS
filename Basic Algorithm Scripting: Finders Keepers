function findElement(arr, func) {
  let num = 0;
  let z;
  for (let i=0; i<arr.length; i++) {
    console.log(arr[i]+'***'+func(arr[i]));
    if (arr[i]==func(arr[i])) {
      z=arr[i];
      return z;
    } else {
      z=undefined;
    }
  } 
  console.log(z);
  console.log(func(0))
  return z;
}

findElement([1, 2, 3, 4], num => num % 2 === 0);
findElement([1, 3, 5, 8, 9, 10], function(num) { return num % 2 === 0; })
