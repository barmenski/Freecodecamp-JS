function sumAll(arr) {
var sum=arr[0]+arr[1];
var diff=arr[1]-arr[0];
if (diff<0) {
  diff=diff*(-1);
  var minItem=arr[1]} else {
    var minItem=arr[0]
  }
for (let i=1; i<diff; i++){
  console.log('sum='+sum+" : "+'minItem='+minItem+" : "+'+1'+" : "+'i='+i);
  sum=sum+minItem+1;
  console.log(sum);
}
  return sum;
}

sumAll([1, 4]);
