function largestOfFour(arr) {
  let arrSort=[];
 for (let i=0; i<=arr.length; i++) {
   for (let j=0; j<=arr[i].length; j++) {
     if (arr[i][j]>arr[i][j+1]) {
       arrSort=arr[i][j];
     } else {
       arrSort=arr[i][j+1];
     }
   }
 }
 console.log(arr[0][0]);
  return arr;

}

largestOfFour([[4, 5, 1, 3], [13, 27, 18, 26], [32, 35, 37, 39], [1000, 1001, 857, 1]]);
